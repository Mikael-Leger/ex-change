{
    "version": 1,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/package.json",
        "use": "@vercel/react"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  