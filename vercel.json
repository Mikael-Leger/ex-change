{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "backend/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/backend/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/backend/index.py"
      }
    ]
  }
  