{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/templates/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.py"
        }
    ]
}